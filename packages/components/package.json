{
  "name": "uipress",
  "version": "0.1.0",
  "description": "The SolidJS component library.",
  "keywords": [
    "solid",
    "solidjs",
    "ui",
    "library",
    "design",
    "system",
    "component",
    "components",
    "frontend",
    "solidjs-component"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/uipress.cjs.js",
  "module": "./dist/uipress.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/uipress.es.js",
      "require": "./dist/uipress.cjs.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "types": "tsc",
    "build:types:cjs": "tsc --module CommonJS --outDir dist/cjs"
  },
  "license": "MIT",
  "author": "Farhad Azarbarzinniaz (https://github.com/farhadniaz)",
  "sideEffects": false,
  "devDependencies": {
    "@types/babel-core": "^6.25.7",
    "@types/node": "^18.7.6",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-dts": "^1.0.4",
    "vite-plugin-solid": "^2.3.0"
  },
  "dependencies": {
    "load-json-file": "^7.0.1",
    "solid-icons": "^1.0.1",
    "solid-js": "^1.4.7",
    "solid-styled-components": "^0.28.4"
  }
}
